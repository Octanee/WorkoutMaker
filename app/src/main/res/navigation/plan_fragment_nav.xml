<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/plan_fragment_nav"
    app:startDestination="@id/planFragment">
    <fragment
        android:id="@+id/planFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.plan.PlanFragment"
        android:label="Plan"
        tools:layout="@layout/fragment_plan">
        <action
            android:id="@+id/action_planFragment_to_macrocycleListFragment"
            app:destination="@id/macrocycleListFragment" />
        <action
            android:id="@+id/action_planFragment_to_createMacrocycleFragment"
            app:destination="@id/createMacrocycleFragment" />
    </fragment>
    <fragment
        android:id="@+id/createMacrocycleFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.createmacrocycle.CreateMacrocycleFragment"
        android:label="Create Macrocycle"
        tools:layout="@layout/fragment_create_macrocycle">
        <action
            android:id="@+id/action_createMacrocycleFragment_to_macrocycleListFragment"
            app:destination="@id/macrocycleListFragment" />
        <action
            android:id="@+id/action_createMacrocycleFragment_to_createMesocycleFragment"
            app:destination="@id/createMesocycleFragment" />

    </fragment>
    <fragment
        android:id="@+id/macrocycleListFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.macrocyclelist.MacrocycleListFragment"
        android:label="Macrocycle list"
        tools:layout="@layout/fragment_macrocycle_list">
        <action
            android:id="@+id/action_macrocycleListFragment_to_planFragment"
            app:destination="@id/planFragment" />
        <action
            android:id="@+id/action_macrocycleListFragment_to_createMacrocycleFragment"
            app:destination="@id/createMacrocycleFragment" />
    </fragment>
    <fragment
        android:id="@+id/createMesocycleFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.createmesocycle.CreateMesocycleFragment"
        android:label="fragment_create_mesocycle"
        tools:layout="@layout/fragment_create_mesocycle">
        <action
            android:id="@+id/action_createMesocycleFragment_to_createMacrocycleFragment"
            app:destination="@id/createMacrocycleFragment" />
        <action
            android:id="@+id/action_createMesocycleFragment_to_createMicrocycleFragment"
            app:destination="@id/createMicrocycleFragment" />

    </fragment>
    <fragment
        android:id="@+id/createTrainingFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.createtraining.CreateTrainingFragment"
        android:label="fragment_create_training"
        tools:layout="@layout/fragment_create_training">
        <action
            android:id="@+id/action_createTrainingFragment_to_addSetsToTrainingFragment"
            app:destination="@id/addSetsToTrainingFragment" />
        <action
            android:id="@+id/action_createTrainingFragment_to_createMicrocycleFragment"
            app:destination="@id/createMicrocycleFragment" />
        <argument
            android:name="trainingWithSetAndExercises"
            android:defaultValue="@null"
            app:argType="com.octaneee.workoutmaker.data.model.relation.TrainingWithSetAndExercises"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/addSetsToTrainingFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.addsetstotraining.AddSetsToTrainingFragment"
        android:label="Add sets"
        tools:layout="@layout/fragment_add_sets_to_training">
        <action
            android:id="@+id/action_addSetsToTrainingFragment_to_createTrainingFragment"
            app:destination="@id/createTrainingFragment" />
        <action
            android:id="@+id/action_addSetsToTrainingFragment_to_exerciseListSortTypeFragment"
            app:destination="@id/exerciseListSortTypeFragment" />
        <argument
            android:name="wholeExercise"
            android:defaultValue="@null"
            app:argType="com.octaneee.workoutmaker.data.model.relation.WholeExercise"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/exerciseListFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.exerciselist.ExerciseListFragment"
        android:label="Exercise list"
        tools:layout="@layout/fragment_exercise_list">
        <action
            android:id="@+id/action_exerciseListFragment_to_addSetsToTrainingFragment"
            app:destination="@id/addSetsToTrainingFragment" />
        <action
            android:id="@+id/action_exerciseListFragment_to_createExerciseFragment"
            app:destination="@id/createExerciseFragment" />
        <argument
            android:name="muscle"
            android:defaultValue="@null"
            app:argType="com.octaneee.workoutmaker.data.model.entity.Muscle"
            app:nullable="true" />
        <argument
            android:name="equipment"
            android:defaultValue="@null"
            app:argType="com.octaneee.workoutmaker.data.model.entity.Equipment"
            app:nullable="true" />
        <argument
            android:name="exerciseType"
            android:defaultValue="@null"
            app:argType="com.octaneee.workoutmaker.data.model.entity.ExerciseType"
            app:nullable="true" />

    </fragment>
    <fragment
        android:id="@+id/createExerciseFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.createexercise.CreateExerciseFragment"
        android:label="Create exercise"
        tools:layout="@layout/fragment_create_exercise">
        <action
            android:id="@+id/action_createExerciseFragment_to_exerciseListFragment"
            app:destination="@id/exerciseListFragment" />
        <argument
            android:name="muscle"
            app:argType="com.octaneee.workoutmaker.data.model.entity.Muscle"
            app:nullable="true" />
        <argument
            android:name="equipment"
            app:argType="com.octaneee.workoutmaker.data.model.entity.Equipment"
            app:nullable="true" />
        <argument
            android:name="exerciseType"
            app:argType="com.octaneee.workoutmaker.data.model.entity.ExerciseType"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/exerciseListSortTypeFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.exerciselistsorttype.ExerciseListSortTypeFragment"
        android:label="Exercise list type"
        tools:layout="@layout/fragment_exercise_list_sort_type">
        <action
            android:id="@+id/action_exerciseListSortTypeFragment_to_exerciseListFragment"
            app:destination="@id/exerciseListFragment" />
    </fragment>
    <fragment
        android:id="@+id/createMicrocycleFragment"
        android:name="com.octaneee.workoutmaker.ui.fragment.plan.createmicrocycle.CreateMicrocycleFragment"
        android:label="fragment_create_microcycle"
        tools:layout="@layout/fragment_create_microcycle">
        <action
            android:id="@+id/action_createMicrocycleFragment_to_createMesocycleFragment"
            app:destination="@id/createMesocycleFragment" />
        <action
            android:id="@+id/action_createMicrocycleFragment_to_createTrainingFragment"
            app:destination="@id/createTrainingFragment" />
    </fragment>
</navigation>